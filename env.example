# Odoo API Configuration
# Copy this file to .env and fill in your actual values

# Odoo Server URL (e.g., https://your-company.odoo.com or http://localhost:8069)
VITE_ODOO_URL=https://your-odoo-instance.com

# Odoo Database Name
VITE_ODOO_DATABASE=your_database_name

# Odoo Username (usually email)
VITE_ODOO_USERNAME=your_username@example.com

# Odoo Password
VITE_ODOO_PASSWORD=your_password

# Optional: Odoo API Key (if using API key authentication)
VITE_ODOO_API_KEY=your_api_key_here

# External Supabase Project Configuration
# For integrating with your other Supabase project that contains invoices and sales_targets tables
NEXT_PUBLIC_EXTERNAL_SUPABASE_URL=https://your-other-project.supabase.co
NEXT_PUBLIC_EXTERNAL_SUPABASE_ANON_KEY=your_other_project_anon_key_here

# Vite Environment Variables (for browser access)
VITE_EXTERNAL_SUPABASE_URL=https://your-other-project.supabase.co
VITE_EXTERNAL_SUPABASE_ANON_KEY=your_other_project_anon_key_here

# Other existing environment variables
# VITE_SUPABASE_URL=your_supabase_url
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key 